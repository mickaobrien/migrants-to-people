{
	"manifest_version": 2,
	"name": "Migrants to People",
	"version": "1.1",
	"description": "Replaces the text 'migrant' with 'desperate and terrified person'.",
    "icons": { "48": "icon48.png",
               "128": "icon128.png" },
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}

